; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Sandboxnator"
config/description="A LAN multiplayer sandbox game, have fun with your friends, hang out, build and destroy :3"
config/version="0.4.0"
run/main_scene="res://UI/MainMenu/MainMenu.tscn"
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
boot_splash/use_filter=false
boot_splash/image="res://UI/SBlogo.png"
config/icon="res://Assets/Textures/sandboxnator_icon.png"

[autoload]

SaveLoader="*res://Source/Common/Utils/IO/SaveLoader.cs"
GameRegistries="*res://GameContent/Registry/GameRegistries.cs"
ScenesBank="*res://World/ScenesBank/ScenesBank.tscn"
PlayerProfileManager="*res://Source/Entities/Player/PlayerProfileManager.cs"
UiSoundManager="*res://UI/UiSoundManager.tscn"
ServerDiscovery="*res://Source/Network/ServerDiscovery/ServerDiscovery.tscn"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Sandboxnator Reinassance Godot"

[editor]

run/main_run_args="client"

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://GameContent/": "yellow",
"res://GameContent/Buildings/": "yellow",
"res://GameContent/Items/": "yellow",
"res://GameContent/Items/ExplosionStaff/": "red",
"res://GameContent/Items/Nut/": "red",
